{
  "name": "martinoconnor/wp-content",
  "description": "Marty's Child Theme",
  "autoload": {
    "psr-4": {
      "Martinoconnor\\WpContent\\": "src/"
    }
  },
  "authors": [
    {
      "name": "martinDolan",
      "email": "martyopants@gmail.com"
    }
  ],
  "require": {
    "wpackagist-plugin/custom-post-type-ui": "1.17.1",
    "wpackagist-plugin/fakerpress": "0.6.6",
    "wpackagist-plugin/create-block-theme": "2.4.0",
    "wpackagist-plugin/ninja-forms": "3.8.16"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
